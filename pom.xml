    <scm>
        <connection>scm:git:https://github.com/lutece-secteur-public/gru-module-taskstack-identity.git</connection>
        <developerConnection>scm:git:https://github.com/lutece-secteur-public/gru-module-taskstack-identity.git</developerConnection>
        <url>https://github.com/lutece-secteur-public/gru-module-taskstack-identity</url>
