    <groupId>fr.paris.lutece.plugins</groupId>

    <artifactId>module-taskstack-identity</artifactId>
    <packaging>lutece-plugin</packaging>

    <version>1.0.1-SNAPSHOT</version>
    <name>Lutece task stack identity module</name>

    <properties>
        <componentName>taskstack-identity</componentName>
        <jiraProjectName>TASKSTACKIDENTITY</jiraProjectName>
        <jiraComponentId />
